/* pop-up */

        var myCallback = function(val) { 
            var form = $('.form-send').closest('form');
            if( form.valid() ) {
                $.ajax({
                    url: '/send.php',
                    type: 'post',
                    data: form.serialize(),
                    success: function(data) {
                        if(data === 'true'){
                            document.location.href = '/thanks.html';
                        }
                        else{
                            document.location.href = '/index.php?status=false';
                        }
                    }
                });
            }
        };   

        $(function(){
            $('.button-koraga-form-1').click(function(){
                console.log($('.koraga-form-1').find('input[name=formlabel]').val($(this).data('label')));
                if ( $(this).hasClass('sale') ) {
                    $('.koraga-form-1').find('h2').addClass('hidden').removeClass('visible');
                    $('.koraga-form-1').find('.sale').removeClass('hidden').addClass('visible');
                }
                else if ( $(this).hasClass('time') ) {
                    $('.koraga-form-1').find('h2').addClass('hidden').removeClass('visible');
                    $('.koraga-form-1').find('.time').removeClass('hidden').addClass('visible');
                }
                else {
                    $('.koraga-form-1').find('h2').addClass('hidden').removeClass('visible');
                    $('.koraga-form-1').find('.default').removeClass('hidden').addClass('visible');
                }
                $('.master-box-w').fadeOut(300);
                $('.price-w').fadeOut(300);
                $('.koraga-form-1').fadeIn(300);
            });
        });

        var recaptcha = true;

        $('.form-send').click(function(){
            var form = $(this).closest('form');
            if( form.valid() ){
                if (recaptcha) {
                    grecaptcha.render('g-recaptcha', {
                      'sitekey' : '6Lc689YUAAAAAC4Q867vnbkF6HlZyh5isS6iJRs_',
                      'callback' : myCallback
                    });
                    recaptcha = false;
                }
                $('#g-recaptcha').removeClass('hidden').addClass('visible');
                $('.submit-form').removeClass('visible').addClass('hidden');
            }
        });

